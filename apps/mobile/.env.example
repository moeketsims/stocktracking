# Supabase Configuration
# Copy this file to .env and update with your values

# Local Development (default)
EXPO_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_local_anon_key_here

# For mobile device testing on same network, use your machine's IP:
# EXPO_PUBLIC_SUPABASE_URL=http://192.168.x.x:54321

# Staging (uncomment for staging)
# EXPO_PUBLIC_SUPABASE_URL=https://your-staging-project.supabase.co
# EXPO_PUBLIC_SUPABASE_ANON_KEY=your_staging_anon_key

# Production (uncomment for production)
# EXPO_PUBLIC_SUPABASE_URL=https://your-production-project.supabase.co
# EXPO_PUBLIC_SUPABASE_ANON_KEY=your_production_anon_key
